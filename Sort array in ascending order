

#include <iostream>

using namespace std;

int main()
{
   int n; 
   cin>>n; 
   int a[n]; 
   for(int i=0;i<n;i++) 
   { 
       cin>>a[i]; 
   } 
   for(int i=0;i<n;i++) 
   { 
       for(int j=i+1;j<n;j++) 
       {  
           if(a[i]>a[j]) 
           { 
               
                int t = a[i]; 
                a[i] = a[j]; 
                a[j] = t; 
           } 
       } 
   } 
   for(int i=0;i<n;i++) 
   { 
       cout<<a[i]<<" "; 
   }
           
       
}


input: 5
       3 5 1 0 2
       
ouptu: 0 1 2 3 5
